# Rename this to avoid the merge conflict with the 'QT:Quote ID' column
df_quotes['Full_Quote_ID_Prefix'] = df_quotes['Document'].str.split('-', n=1).str[0]


# Sort using the new column name
df_quotes = df_quotes.sort_values(by=['Full_Quote_ID_Prefix', 'Review History'], ascending=[True, False])
# Drop duplicates using the new column name
df_quotes = df_quotes.drop_duplicates(subset='Full_Quote_ID_Prefix', keep='first')
